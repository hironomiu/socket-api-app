generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  nickname String @db.VarChar(100)
  email    String @unique(map: "email") @db.VarChar(100)
  password String @db.VarChar(100)
}
